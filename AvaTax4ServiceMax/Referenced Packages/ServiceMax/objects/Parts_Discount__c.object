    <validationRules>
        <fullName>SVMX_Product_Validation</fullName>
        <active>true</active>
        <description>Product Validation</description>
        <errorConditionFormula>IF(LEN(Product__r.Id) &gt; 0, IF(OR(LEN(TEXT(Product_Line__c)) &gt; 0, LEN(TEXT(Product_Family__c)) &gt; 0), TRUE, FALSE), IF(LEN(TEXT(Product_Line__c)) &gt; 0, IF(LEN(TEXT(Product_Family__c)) &gt; 0, TRUE, FALSE), IF(LEN(TEXT(Product_Family__c)) &gt; 0, FALSE, TRUE)))</errorConditionFormula>
        <errorMessage>Discount must be applied on only one of Product, Product Family, or Product Line.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SVMX_ServicePlan_Or_Contract</fullName>
        <active>true</active>
        <errorConditionFormula>IF(LEN(Service_Contract__r.Id) &gt; 0, IF(LEN( Service_Plan__r.Id ) &gt; 0, TRUE, FALSE), IF(LEN(Service_Plan__r.Id) &gt; 0, FALSE, TRUE))</errorConditionFormula>
        <errorMessage>Parts Discount must be associated with either Service Plan or Service Contract, but not both.</errorMessage>
    </validationRules>
